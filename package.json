{
	"name": "ebay_lister",
	"productName": "eBay Lister",
	"version": "1.0.0",
	"description": "Streamlines the process of listing an item on eBay.",
	"repository": "https://github.com/jwrand16/eBay-Lister",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --out=release-builds",
		"build-win": "electron-packager . ebay_lister --overwrite --platform=win32 --arch=x64 --icon=icons/win/icon.ico --out=release-builds"
	},
	"dependencies": {
		"dotenv": "^8.2.0"
	},
	"author": "James Randolph",
	"license": "ISC",
	"devDependencies": {
		"electron": "^18.3.7",
		"electron-builder": "^22.6.1",
		"electron-packager": "^14.1.1"
	}
}
